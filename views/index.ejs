<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
   <% messages.forEach(function(message) { %>
    <div style="display: flex; border:2px solid black; padding:2vw 2vh">
      <h3><%= message.text %> - </h3>
      <h4><%= message.user %></h4>
    </div>
    <% }) %>

    <a href="/new">new Message </a>
  </body>
</html>
